@model IEnumerable<MailParserResponseDto>

<!DOCTYPE html>
<html>
<head>
    <title>Office 365 Mail Parser</title>
</head>
<body>
    <form method="post">
        <fieldset>
            <legend>Enter Details</legend>
            <div>
                <label for="TenantId">Tenant Id:</label>
                <input type="text" name="TenantId" value="" />
            </div>
            <div>
                <label for="ClientId">Client Id:</label>
                <input type="text" name="ClientId" value="" />
            </div>
            <div>
                <label for="UserName">UserName:</label>
                <input type="text" name="UserName" value="" />
            </div>
            <div>
                <label for="Password">Password:</label>
                <input type="password" name="Password" value="" />
            </div>
            <div>
                <label for="StartTime">Start Time:</label>
                <input type="text" name="StartTime" value="" />
            </div>
            <div>
                <label for="EndTime">End Time:</label>
                <input type="text" name="EndTime" value="" />
            </div>
            <div>
                <label for="SubjectPattern">Subject Pattern:</label>
                <input type="text" name="SubjectPattern" value="" />
            </div>
            <div>
                <label>&nbsp;</label>
                <input type="submit" value="Submit" class="submit" />
            </div>
        </fieldset>
    </form>



    <table>
        <tr>
            <th>Date Received</th>
            <th>Sender</th>
            <th>Subject</th>
        </tr>
        @foreach ( var item in Model )
        {
            <tr>
                <td>
                    @Html.DisplayFor( modelItem => item.ReceivedDateTime )
                </td>
                <td>
                    @Html.DisplayFor( modelItem => item.Sender )
                </td>
                <td>
                    @Html.DisplayFor( modelItem => item.Subject )
                </td>
            </tr>
        }
    </table>
</body>
</html>